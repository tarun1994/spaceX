{
  "name": "space-x-program",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "ng run spaceX-program:prerender",
    "optimizecss": "gulp --gulpfile optimize-css.gulpfile.js",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod --aot --build-optimizer && ng run spaceX-program:server:production --bundleDependencies all",
    "start:heroku": "node dist/server",
    "postinstall": "npm run build:ssr"
  },
  "private": true,
